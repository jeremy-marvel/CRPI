&ACCESS RV
&REL 6
&PARAM TEMPLATE = C:\KRC\Roboter\Template\FunctionVorgabe
&PARAM EDITMASK = *
DEFFCT FRAME STARTPOINT(DEGREES:IN )
INT DEGREES
DECL FRAME POINT

POINT.A=1.17
POINT.B=0
POINT.C=-180
POINT.Y=-560

IF ((DEGREES==10) OR (DEGREES==20)) THEN
  POINT.X=12.3
  IF DEGREES==10 THEN
    POINT.Z=61.2
  ELSE
    POINT.Z=56.58
  ENDIF
ELSE
  POINT.X=10.74
  IF DEGREES==5 THEN
    POINT.Z=63.72
  ELSE
    POINT.Z=59.36
  ENDIF
ENDIF

RETURN POINT

ENDFCT