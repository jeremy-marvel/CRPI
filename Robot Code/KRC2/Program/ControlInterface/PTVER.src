&ACCESS RVO1
&REL 4
&PARAM TEMPLATE = C:\KRC\Roboter\Template\FunctionVorgabe
&PARAM EDITMASK = *
DEFFCT BOOL PTVER( POINT:OUT )
; Verifies that point is valid

FRAME POINT
DECL BOOL XVER, YVER, ZVER

XVER = (POINT.X < 750) AND (POINT.X > -750)
YVER = (POINT.Y < 750) AND (POINT.Y > -750)
ZVER = POINT.Z > 0

RETURN(XVER AND YVER AND ZVER)

ENDFCT